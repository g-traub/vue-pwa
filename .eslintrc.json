{
  "env": {
    "node": true
  },
  "plugins": ["prettier"],
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "prettier/vue",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "workbox": true,
    "clients": true
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
